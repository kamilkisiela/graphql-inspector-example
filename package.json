  "scripts": {
    "precommit": "graphql-inspector introspect schema.js --write schema.graphql"
  },
  "dependencies": {
    "@graphql-inspector/cli": "0.6.5",
    "graphql": "14.0.2",
    "husky": "1.3.1"
  },
  "devDependencies": {
    "source-map-support": "0.5.9"
  },
  "graphql-inspector": {
    "diff": true,
    "schema": {
      "ref": "master",
      "path": "schema.graphql"
    }
  }
}